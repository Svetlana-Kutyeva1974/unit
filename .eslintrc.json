{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
     "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-multiple-empty-lines": "warn",
        "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
        ],
        "import/extensions": [
        "error",
        "ignorePackages"
        ],
        "import/no-unresolved": [
      1,
      {
        "commonjs": false,
        "amd": false
      }
    ]
    },
    "settings": {
    "import/resolver": {
      "node": {
        "path": ["src"]
      }
    }
  }
}